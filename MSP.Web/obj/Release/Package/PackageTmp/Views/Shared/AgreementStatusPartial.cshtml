@model Nullable<int>

@if (Model == 0)
{
    <span class="label label-default" data-toggle="tooltip" data-placement="right" data-original-title="草稿">草稿</span>
}
else if (Model == 1)
{
    <span class="label label-success" data-toggle="tooltip" data-placement="right" data-original-title="已提交">已提交</span>
}
else if (Model == 2)
{
    <span class="label label-inverse" data-toggle="tooltip" data-placement="right" data-original-title="已归档">已归档</span>
}
else if (Model == 3)
{
    <span class="label label-primary" data-toggle="tooltip" data-placement="right" data-original-title="已作废">已作废</span>
}
else if (Model == 4)
{
    <span class="label label-danger" data-toggle="tooltip" data-placement="right" data-original-title="未通过">未通过</span>
}
else if (Model == 5)
{
    <span class="label label-warning" data-toggle="tooltip" data-placement="right" data-original-title="审批中">审批中</span>
}
else if (Model == 6)
{
    <span class="label label-primary" data-toggle="tooltip" data-placement="right" data-original-title="审批中">已变更</span>
}
else
{
    <span class="label label-warning" data-toggle="tooltip" data-placement="right" data-original-title="未知">未知</span>
}