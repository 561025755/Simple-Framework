
@model MSP.Models.Domain.ContactModel

@{
    ViewBag.Title = "联系人详细";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section PageTitle{
    联系人详细 <small>点击 @Html.ActionLink("这里", "Index") 回到列表</small>
}

<div class="row">
    <!-- begin col-12 -->
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div class="panel panel-info">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-repeat"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
                </div>
                <h4 class="panel-title">详细信息</h4>
            </div>
            <div class="panel-body">
                <div class="form-group">
                    <div class="row">
                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.Name)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.Name)
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.CustomerId)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.CustomerName)
                            </div>
                        </div>

                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.Business)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.Business)
                            </div>
                        </div>

                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.Department)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.Department)
                            </div>
                        </div>

                    </div>
                </div>

                <div class="form-group">
                    <div class="row">
                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.QQ)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.QQ)
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.WebChat)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.WebChat)
                            </div>
                        </div>

                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.Phone)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.Phone)
                            </div>
                        </div>

                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.Mobile)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.Mobile)
                            </div>
                        </div>

                    </div>
                </div>

                <div class="form-group">
                    <div class="row">
                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.Fax)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.Fax)
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.Email)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.Email)
                            </div>
                        </div>

                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.Address)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.Address)
                            </div>
                        </div>

                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.Description)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.Description)
                            </div>
                        </div>

                    </div>
                </div>

                <div class="form-group">
                    <div class="row">
                        <div class="col-sm-3">
                            <label>
                                最后一次更新时间
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.CreateTime)
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <label>
                                @Html.DisplayNameFor(model => model.UserName)
                            </label>
                            <div>
                                @Html.DisplayFor(model => model.UserName)
                            </div>
                        </div>
                    </div>
                </div>

                @Html.ActionLink("编辑", "Edit", new { id = Model.Id }, new { @class = "btn btn-sm btn-success" })

            </div>
        </div>
        <!-- end panel -->
    </div>
    <!-- end col-12 -->
</div>