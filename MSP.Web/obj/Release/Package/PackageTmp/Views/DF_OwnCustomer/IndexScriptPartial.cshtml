
<script type="text/javascript">
            function opneModel() {
                $("#modal-customer-analyze").modal('show');
                $('#data-table').dataTable().fnDestroy();
            }

            function searchAnalyzeCustomer() {
                var keyword = $('#txtSearchKeyword').val();

                $('#data-table').dataTable().fnDestroy();
                $('#data-table').dataTable({
                    "processing": true,
                    "serverSide": true,
                    "pageLength": 8,
                    "lengthChange": false,
                    "ordering": true,
                    "info": true,
                    "bAutoWidth": false,
                    "pagingType": "full",
                    "searching": false,
                    "ajax": {
                        "url": '/DF_OwnCustomer/CustomerAnalyze',
                        "data": function (d) {
                            d.keyword = keyword
                        }
                    },
                    "columns": [
                        //{ "data": "CustomerCode" },
                        { "data": "Name" },
                        { "data": "Remark" },
                        { "data": "FilingStatusName" }
                    ],
                    language: {
                        lengthMenu: '<select class="form-control input-xsmall">' + '<option value="5">5</option>' + '<option value="10">10</option>' + '<option value="20">20</option>' + '<option value="30">30</option>' + '<option value="40">40</option>' + '<option value="50">50</option>' + '</select>条记录',//左上角的分页大小显示。
                        processing: "载入中",//处理页面数据的时候的显示
                        paginate: {//分页的样式文本内容。
                            previous: "上一页",
                            next: "下一页",
                            first: "第一页",
                            last: "最后一页"
                        },
                        zeroRecords: "没有内容",//table tbody内容为空时，tbody的内容。
                        //下面三者构成了总体的左下角的内容。
                        info: "共_PAGES_ 页，显示第_START_ 到第 _END_ ，_TOTAL_ 项 ",//左下角的信息显示，大写的词为关键字。
                        infoEmpty: "0条记录",//筛选为空时左下角的显示。
                        infoFiltered: ""//筛选之后的左下角筛选提示(另一个是分页信息显示，在上面的info中已经设置，所以可以不显示)，
                    }
                });
            }

            function openQueueDetails(name, orgcode, uid) {
                $('#modal-customer-list').modal('show');
                $('#data-table-queue').dataTable().fnDestroy();
                var mediaType = $('#MediaType').val();

                $('#data-table-queue').dataTable({
                    "processing": true,
                    "serverSide": true,
                    "pageLength": 8,
                    "lengthChange": false,
                    "ordering": true,
                    "info": true,
                    "bAutoWidth": false,
                    "pagingType": "full",
                    "searching": false,
                    "ajax": {
                        "type": "POST",
                        "url": '/DF_OwnCustomer/CustomerQueue',
                        "data": function (d) {
                            d.name = name,
                            d.orgcode = orgcode,
                            d.userId = uid
                        }
                    },
                    "columns": [
                        {
                            "data": function (e) {
                                if (e["UserId"] == uid) {
                                    return '<span class="text-danger">' + e["CustomerCode"] + '</span>';
                                } else {
                                    return '<span>' + e["CustomerCode"] + '</span>';
                                }
                            }
                        },
                        {
                            "data": function (e) {
                                if (e["UserId"] == uid) {
                                    return '<span class="text-danger">' + e["Name"] + '</span>';
                                } else {
                                    return '<span>' + e["Name"] + '</span>';
                                }
                            }
                        },
                        {
                            "data": function (e) {
                                if (e["UserId"] == uid) {
                                    return '<span class="text-danger">' + e["Remark"] + '</span>';
                                } else {
                                    return '<span>' + e["Remark"] + '</span>';
                                }
                            }
                        },
                        {
                            "data": function (e) {
                                if (e["UserId"] == uid) {
                                    return '<span class="text-danger">' + e["FilingStatusName"] + '</span>';
                                } else {
                                    return '<span>' + e["FilingStatusName"] + '</span>';
                                }
                            }
                        }

                    ],
                    language: {
                        lengthMenu: '<select class="form-control input-xsmall">' + '<option value="5">5</option>' + '<option value="10">10</option>' + '<option value="20">20</option>' + '<option value="30">30</option>' + '<option value="40">40</option>' + '<option value="50">50</option>' + '</select>条记录',//左上角的分页大小显示。
                        processing: "载入中",//处理页面数据的时候的显示
                        paginate: {//分页的样式文本内容。
                            previous: "上一页",
                            next: "下一页",
                            first: "第一页",
                            last: "最后一页"
                        },
                        zeroRecords: "没有内容",//table tbody内容为空时，tbody的内容。
                        //下面三者构成了总体的左下角的内容。
                        info: "共_PAGES_ 页，显示第_START_ 到第 _END_ ，_TOTAL_ 项 ",//左下角的信息显示，大写的词为关键字。
                        infoEmpty: "0条记录",//筛选为空时左下角的显示。
                        infoFiltered: ""//筛选之后的左下角筛选提示(另一个是分页信息显示，在上面的info中已经设置，所以可以不显示)，
                    }
                });
            }
</script>