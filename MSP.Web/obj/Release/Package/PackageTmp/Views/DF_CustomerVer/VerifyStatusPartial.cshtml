
@model MSP.Core.Domain.Cus_Customer

@if (Model.Status == 1)
{
    <span class="label label-primary" data-toggle="tooltip" data-placement="right" data-original-title="请求审核">请求审核</span>
}
else if (Model.Status == 2)
{
    <span class="label label-danger" data-toggle="tooltip" data-placement="right" data-original-title="审核未通过">审核未通过</span>
}
else if (Model.Status == 3)
{
    <span class="label label-info" data-toggle="tooltip" data-placement="right" data-original-title="排队中">排队中</span>
}
else if (Model.Status == 4)
{
    <span class="label label-info" data-toggle="tooltip" data-placement="right" data-original-title="进入保护期">进入保护期</span>
}
else if (Model.Status == 5)
{
    <span class="label label-success" data-toggle="tooltip" data-placement="right" data-original-title="已签约">已签约</span>
}
else if (Model.Status == 6)
{
    <span class="label label-danger" data-toggle="tooltip" data-placement="right" data-original-title="审核中">审核中</span>
}
else if (Model.Status == -1)
{
    <span class="label label-danger" data-toggle="tooltip" data-placement="right" data-original-title="已取消">已取消</span>
}
else
{
    <span class="label label-warning" data-toggle="tooltip" data-placement="right" data-original-title="未知">未知</span>
}